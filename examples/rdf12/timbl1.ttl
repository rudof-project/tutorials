prefix : <http://example.org/>
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

:timbl a :Human ;
   rdfs:label "Tim Berners Lee" ;
   :employer :CERN {| :start "1984" ;
                      :end   "1994" |}
                   {| :start "1980" ;
                      :end   "1980" |} .

:CERN a :Organization ;
      rdfs:label "CERN" .

